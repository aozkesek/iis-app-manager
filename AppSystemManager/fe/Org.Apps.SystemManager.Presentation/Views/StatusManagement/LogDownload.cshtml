    PARTIAL-VIEW
    
    *@

@using Org.Apps.BaseService;
@using Org.Apps.BaseDao.Model.Response;
@using Org.Apps.LogManager.Data.Model;

@model AppsModule

<table id="logFilesTable" class="display compact">
    <thead>
        <tr>
            <th>@Resources.Global.Download</th>
        </tr>
    </thead>

    <tbody>
        @if (ViewBag.Result == Result.OK)
        {
            foreach (AppsLogFile logFile in ViewBag.ResultList)
            {
                <tr>
                    <td>
                        <a href="@logFile.LogFileContent">@logFile.LogFileName</a>
                    </td>

                </tr>
            }
        }
    </tbody>

</table>

<script type="text/javascript">

    $("#logFilesTable").DataTable({
        language: DataTableLanguage
        
    });


</script>
